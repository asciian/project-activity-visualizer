{
    "manifest_version": 2,
    "name": "Project Activity Visualizer",
    "version": "0.2",
    "description": "Visualize project activity in Github",
    "homepage_url": "https://github.com/ta2gch/ProjectActivityVisualizer",
    "icons": {
        "48": "icons/icon-48.png",
        "96": "icons/icon-96.png"
    },
    "content_scripts": [
        {
            "matches": ["*://github.com/*"],
            "js": ["content-script.js"]
        }
    ]
}
